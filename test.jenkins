pipeline {
		agent any
		stages{
		    
			stage('Git Check-out'){
				steps{
				git 'https://github.com/thockin/test.git'
				sh"git rev-parse --short HEAD"
				}
			}
				stage('Commmit Code'){
					steps{
					sh"sha=echo ${env.GIT_COMMIT}| cut -c6"
					sh"echo ${env.GIT_COMMIT}"
					sh"echo ${sha}"
					
					sh"echo ${env.BRANCH_NAME}"
					}
				}
			}
	}
